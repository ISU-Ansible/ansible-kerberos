{% macro section(name) -%}
{%   for key, value in name.iteritems() %}
{%     if value is string or value is number %}
{{ key | replace("__",".") }} = {{ value }}
{%     else %}
{{ key | replace("__",".") }} = "{"
{%       for i in value %}
  {{ value }}
{%       endfor %}
"}"	
{%     endif %}
{%   endfor %}
{%- endmacro %}